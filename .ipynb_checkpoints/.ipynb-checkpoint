   "source": [
    "ATL = pd.read_csv(\"Southwest_ATL.csv\")\n",
    "ATL2 = ATL.rename(columns={'Origin': 'Origin_Airport'})\n",
    "ATLdf = ATL2.rename(columns={'Destination Airport': 'Destination_Airport'})\n",
    "\n",
    "Destination_booleans = []\n",
    "for airport in ATLdf.Destination_Airport:\n",
    "    if airport == \"SFO\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"LAX\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"DEN\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"LAS\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"PDX\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"SEA\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"DFW\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"MCO\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"ORD\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"JFK\":\n",
    "        Destination_booleans.append(True)\n",
    "    elif airport == \"ATL\":\n",
    "        Destination_booleans.append(True)\n",
    "    else:\n",
    "        Destination_booleans.append(False)\n",
    "\n",
    "is_long = pd.Series(Destination_booleans)\n",
    "\n",
    "ATL_df = ATLdf[is_long]\n",
    "ATL_df.shape"
   ]
  },
